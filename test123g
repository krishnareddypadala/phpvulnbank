  stages {

    // Build
    stage('fortify1') {
      agent {
        label 'master'
      }
      steps {
        deleteDir()
        checkout scm
      }
    }

    // Static Code Analysis
    stage('fortify2') {
      agent {
        label 'WinBaseMachine'
      }
      steps {
        deleteDir()
        checkout scm
        sh "echo 'Run Static Code Analysis'"
      }
    }

  }

